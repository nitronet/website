{% block title %}Fwk - {% endblock title %}

{% extends "layouts/main.twig" %}

{% block contents %}
<div class="section jumbotron">
  <div class="container">
      <h3 style="margin:0;padding:0">Fwk</h3>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-lg-8 col-md-8">
        {% block fwk_contents %}{% endblock %}
    </div>
    <div class="col-lg-4 col-md-4" style="">
        <form class="form-horizontal">
            <div class="form-group">
              <div class="col-lg-12">
                <input class="form-control" id="search" placeholder="Search Documentation" type="text">
              </div>
            </div>
        </form>
    </div>
  </div>
</div>
{% endblock %}